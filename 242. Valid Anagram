class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        sdic = {}
        tdic = {}

        for i in s:
            if i in sdic:
                sdic[i] += 1
            else:
                sdic[i] = 1

        for j in t:
            if j in tdic:
                tdic[j] += 1
            else:
                tdic[j] = 1
        
        return True if sdic == tdic else False
