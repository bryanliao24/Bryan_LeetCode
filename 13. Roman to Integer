int call(char c){
    switch(c){
        case 'I':return 1;
        case 'V':return 5;
        case 'X':return 10;
        case 'L':return 50;
        case 'C':return 100;
        case 'D':return 500;
        case 'M':return 1000;
    }    
    return 0;
}

int romanToInt(char * s){
   int i,total,temp=0;
   for(i=0;i<strlen(s);i++){
       total+=call(s[i]);
   }
    return total;
}
