def quicksort(arr):
    if len(arr) <= 1:
        return arr
    pivot = arr[len(arr) // 2] 
    left = [x for x in arr if x < pivot]    
    mid = [x for x in arr if x == pivot]
    right = [x for x in arr if x > pivot]
    return quicksort(left) + pivot + quicksort(right)

nums = [5, 1, 9, 3, 7, 2, 8]
print(quicksort(nums))
