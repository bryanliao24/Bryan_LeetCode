#题目大意
Input: nums = [0, 1, 3, 50, 75], lower = 0 and upper = 99,
Output: ["2", "4->49", "51->74", "76->99"]


ans = []
i = lower - 1
v = 0
for end in nums:
    v = end - i
    if v == 2:
        ans.append(str(i+1))
    elif v > 2:
        ans.append("{ii}->{endd}".format(ii=str(i+1),endd=str(end-1)))
    i = end
print(i)
v = upper - i
if v == 1:
    ans.append(str(upper))
elif v > 1:
    ans.append("{ii}->{endd}".format(ii=str(i+1),endd=str(upper)))
