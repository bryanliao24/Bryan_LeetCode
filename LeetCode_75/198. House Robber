# 先求 i - 1 位置和 i - 2 位置的结果比较大小，再求 i 位置的结果

class Solution:
    def rob(self, nums: List[int]) -> int:
        if not nums:
            return 0
        if len(nums) == 1:
            return nums[0]
        dp = [0] * len(nums)
        dp[0] = nums[0]
        dp[1] = max(nums[0], nums[1])
        for i in range(2, len(nums)):
          dp[i] = max(dp[i - 1], dp[i - 2] + nums[i])
        return dp[-1]
-----------------------------------------------------------
class Solution(object):
    def rob(self, nums): 
        prev = 0 
        cur = 0 
        for money in nums:
            prev, cur = cur, max(prev+money, cur)
           
        return cur
