class Solution(object):
    def canConstruct(self, ransomNote, magazine):
        """
        :type ransomNote: str
        :type magazine: str
        :rtype: bool
        """
        ransomCount = collections.Counter(ransomNote)
        magaCount = collections.Counter(magazine)

        for i, v in ransomCount.items() :
            if v > magaCount[i]:
                return False
        
        return True
